     <script language="JavaScript">
      function makePOSTRequest_s_maj_stats(url, parameters) {
      http_request_s_maj_tags = false;
      if (window.XMLHttpRequest) { // Mozilla, Safari,...
         http_request_s_maj_tags = new XMLHttpRequest();
         if (http_request_s_maj_tags.overrideMimeType) {
            http_request_s_maj_tags.overrideMimeType('text/html');
         }
      } else if (window.ActiveXObject) { // IE
         try {
            http_request_s_maj_tags = new ActiveXObject("Msxml2.XMLHTTP");
         } catch (e) {
            try {
               http_request_s_maj_tags = new ActiveXObject("Microsoft.XMLHTTP");
            } catch (e) {}
         }
      }
      if (!http_request_s_maj_tags) {
         alert('Cannot create XMLHTTP instance');
         return false;
      }


      http_request_s_maj_tags.open('POST', url, true);
      http_request_s_maj_tags.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
      http_request_s_maj_tags.send(parameters);

      }


   function set_recherche_st(id,item_recherche, id_oeuvre, url) {


      var poststr = "id=" + encodeURI( id ) +
                    "&item_recherche=" + encodeURI( item_recherche ) +
                    "&id_oeuvre=" + encodeURI( id_oeuvre ) +
                    "&url=" + encodeURI( url ) ;
      if (id>-1)
      	makePOSTRequest_s_maj_stats('/post_recherche_st.php', poststr);

   }
     </script>
